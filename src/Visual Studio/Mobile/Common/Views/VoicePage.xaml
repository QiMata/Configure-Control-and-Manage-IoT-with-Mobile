<?xml version="1.0" encoding="utf-8" ?>
<views:BaseContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:QiMata.ConfigureControlManage.Views;assembly=QiMata.ConfigureControlManage"
             x:Class="QiMata.ConfigureControlManage.Views.VoicePage"
            Title="Voice"
            BindingContext="{Binding Source={StaticResource ViewModelLocator},Path= VoiceViewModel}"
            BackgroundColor="Black">
    <ContentPage.ToolbarItems>
        <ToolbarItem Command="{Binding ListenCommand}" Text="Listen"></ToolbarItem>
    </ContentPage.ToolbarItems>
</views:BaseContentPage>